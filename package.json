  "name": "isp-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "rebuild:better-sqlite3": "electron-rebuild -f -w better-sqlite3 --run-pre-gyp",
    "nxe:build:frontend": "nx build angular-app",
    "nxe:build:backend": "nx build electron-app",
    "nxe:serve:frontend": "nx serve angular-app",
    "nxe:serve:backend": "nx serve electron-app",
    "nxe:test:frontend": "nx test angular-app",
    "nxe:test:backend": "nx test electron-app",
    "nxe:package:app": "nx run electron-app:make --prepackageOnly",
    "nxe:make:app": "nx run electron-app:make",
    "prepack": "npm run nxe:build:frontend && npm run nxe:build:backend",
    "package": "npm run prepack && npm run nxe:package:app",
    "make": "npm run package && npm run nxe:make:app",
    "build:all": "nx build api-server --configuration=production && nx build angular-app --configuration=production && nx build electron-app --configuration=production",
    "package:R": "npm run build:all && nx run electron-app:package",
    "start:dev": "concurrently \"nx serve angular-app --hmr\" \"wait-on http://localhost:4200 && nx serve electron-app\""
  },
